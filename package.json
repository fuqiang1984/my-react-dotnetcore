{
  "name": "webpack-demo",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start:client": "webpack-dev-server --config webpack.client.js --hot  --content-base public --history-api-fallback",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "start:dev": "webpack-dev-server --hot",
    "dev:server": "node build/serverbundle.js",
    "dev:build-server": "webpack --config webpack.server.js",
    "dev:build-client": "webpack --config webpack.client.js",
    "prod:bundlefordeploy": "webpack --config webpack.client.js",
    "prod:cleanwwwroot": "rimraf ../smartleader/WebApp/wwwroot",
    "prod:copytowwwroot": "cpx 'public/**/*' ../smartleader/WebApp/wwwroot",
    "prod:deploy": "npm-run-all --serial prod:bundlefordeploy prod:cleanwwwroot prod:copytowwwroot",
    "jsonserver": "nodemon jsonserver.js",
    "start": "npm-run-all --parallel start:client jsonserver"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
